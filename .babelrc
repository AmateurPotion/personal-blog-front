{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": ["./components"],
          "@public": ["./public"]
        },
        "extensions": [
          ".wasm",
          ".mjs",
          ".js",
          ".jsx",
          ".json",
          ".ts",
          ".tsx",
          ".jpg",
          ".png"
        ]
      }
    ]
  ]
}
