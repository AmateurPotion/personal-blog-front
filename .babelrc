    "plugins": ["babel-plugin-styled-components", [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "@components": ["./components"]
          },
          "extensions": [ ".wasm", ".mjs", ".js", ".jsx", ".json", ".ts", ".tsx" ]
        }
    ]],
}